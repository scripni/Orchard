<system.diagnostics>
  <trace autoflush="true"/>
  <sources>
    <source name="Default" switchValue="Verbose">
      <listeners>
        <add name="CaptureTraceMessages" />
      </listeners>
    </source>
  </sources>
  <sharedListeners>
    <add name="CaptureTraceMessages" type="Orchard.Specs.Hosting.HostingTraceListener, Orchard.Specs" initializeData="" />
  </sharedListeners>
</system.diagnostics>
